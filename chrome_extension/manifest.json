{
	"manifest_version": 3,
	"name": "Example Extension",
	"version": "1.0",
	"description": "Chrome extension skeleton",
	"action": {
	"default_popup": "popup.html"
	},
	"background": {
	"service_worker": "background.js"
	},
	"permissions": [],
	"web_accessible_resources": [
	{
	"resources": [
	"dictionaries/*.dic",
	"dictionaries/*.aff"
	],
	"matches": [
	"<all_urls>"
	]
	}
	]
}
